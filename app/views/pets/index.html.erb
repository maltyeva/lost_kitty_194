<h1>Lost Pets</h1>
<div class="container">
  <div class="row">
    <% @pets.each do |pet|%>
      <div class="col col-xs-12 col-sm-4">
        <%= link_to pet.species, pet_path(pet) %>
        <p><%= pet.species %>,<%= pet.location %><%= pet.gender %></p>
      </div>
    <%end%>
  </div>
  <%= link_to "Add a Pet", new_pet_path, class: "btn btn-primary"  %>
</div>